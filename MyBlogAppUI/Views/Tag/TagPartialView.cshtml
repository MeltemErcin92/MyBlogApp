@using MyBlog.Data.Model
@model List<Tag>
<div class="sidebox widget">
    <h3 class="widget-title section-title">Tags</h3>
    <ul class="tag-list">
        @foreach (Tag t in Model)
        {
            <li><a href="Article/Detail/@t.TagId">@t.TagName</a></li>
        }


    </ul>
    <!-- /.tag-list -->

</div>